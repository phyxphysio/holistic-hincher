{
  "web": {
    "image": "$IMAGE_WEB",
    "ports": {
      "8000": "HTTP"
    },
    "environment": {
      "DB_HOST": "$DB_HOST",
      "DB_DATABASE": "$DB_DATABASE",
      "DB_USERNAME": "$DB_USERNAME",
      "DB_PASSWORD": "$DB_PASSWORD",
      "SECRET_KEY": "$SECRET_KEY",
